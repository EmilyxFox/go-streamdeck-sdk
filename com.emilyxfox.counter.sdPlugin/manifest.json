{
    "$schema": "https://schemas.elgato.com/streamdeck/plugins/manifest.json",
    "Software": {
        "MinimumVersion": "6.4"
    },
    "Name": "GoLang Plugin",
    "Author": "EmilyxFox",
    "Version": "0.2.0.0",
    "Category": "Go",
    "CategoryIcon": "img/go-logo",
    "Description": "A trial GoLang StreamDeck plugin.",
    "UUID": "com.emilyxfox.counter",
    "SDKVersion": 2,
    "OS": [{"Platform": "windows", "MinimumVersion": "10"}],
    "CodePath": "plugin.exe",
    "Icon": "img/marketplace",
    "Actions": [
        {
            "Icon": "img/defaultImage",
            "Name": "Counter",
            "States": [
                {
                    "Image": "img/defaultImage",
                    "TitleAlignment": "middle",
                    "FontSize": 16
                }
            ],
            "Controllers": ["Keypad"],
            "Tooltip": "How many times did you get pwned today? Keep track with this counter.",
            "UUID": "com.emilyxfox.counter.counter"
            
        }
    ]
}